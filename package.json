{
  "name": "@anpigon/n8n-nodes-youtube-transcript",
  "version": "0.1.8",
  "description": "download transcript of youtube videos in your n8n workflows",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "youtube",
    "transcript",
    "youtube-transcript"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "anpigon",
    "email": "markan82@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpigon/n8n-nodes-youtube-transcript.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/YoutubeTranscriptNode/YoutubeTranscriptNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "youtube-transcript": "^1.2.1"
  }
}
